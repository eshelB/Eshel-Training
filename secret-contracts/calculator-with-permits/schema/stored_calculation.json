{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "StoredCalculation",
  "type": "object",
  "required": [
    "left_operand",
    "operation",
    "result"
  ],
  "properties": {
    "left_operand": {
      "type": "integer",
      "format": "int64"
    },
    "operation": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "result": {
      "type": "integer",
      "format": "int64"
    },
    "right_operand": {
      "type": [
        "integer",
        "null"
      ],
      "format": "int64"
    }
  }
}
